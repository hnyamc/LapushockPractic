<Page x:Class="LapushockPractic.Pages.NavigationPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LapushockPractic.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="NavigationPage">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="79*"/>
            <RowDefinition Height="371*"/>
        </Grid.RowDefinitions>
        <TextBlock Style="{DynamicResource H1}">Страница навигации</TextBlock>
        <StackPanel Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Margin="5" Click="Button_Click_Product">Продукты</Button>
            <Button Margin="5" Click="Button_Click_Material">Материалы</Button>
        </StackPanel>
    </Grid>
</Page>
